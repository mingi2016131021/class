   "source": [
    "class Solution(object):\n",
    "    def reverseStr(self, s, k):\n",
    "        \n",
    "        s = list(s)\n",
    "        for i in range(0,len(s),2*k): \n",
    "            s[i:i+k] = s[i:i+k][::-1]\n",
    "        return \"\".join(s)"
   ]
