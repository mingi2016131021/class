   "source": [
    "class Solution(object):\n",
    "    def smallestRangeI(self, nums, k):\n",
    "        maxi = max(nums)\n",
    "        mini = min(nums)\n",
    "        score = (maxi - k) - (mini + k)\n",
    "        return max(score, 0)\n"
   ]
  }
