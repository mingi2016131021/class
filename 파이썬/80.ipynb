   "source": [
    "class Solution(object):\n",
    "    def maxDepth(self, root):\n",
    "        if not root:\n",
    "            return 0\n",
    "        \n",
    "        depth = 0\n",
    "        for c in root.children:\n",
    "            depth = max(depth, self.maxDepth(c))\n",
    "            \n",
    "        return depth+1"
   ]
