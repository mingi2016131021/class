    "class Solution(object):\n",
    "    def uncommonFromSentences(self, s1, s2):\n",
    "        s = s1.split() + s2.split()\n",
    "        res = []\n",
    "        for i in s:\n",
    "            if s.count(i) == 1:\n",
    "                res.append(i)\n",
    "        return res"
   ]
