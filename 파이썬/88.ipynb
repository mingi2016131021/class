   "source": [
    "class Solution:\n",
    "    def checkAlmostEquivalent(self, word1, word2):\n",
    "        for alph in (set(word1)|set(word2)):\n",
    "            if max(word1.count(alph),word2.count(alph))-min((word1.count(alph),word2.count(alph)))>3:\n",
    "                return False\n",
    "        return True"
   ]
