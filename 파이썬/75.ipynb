   "source": [
    "class Solution(object):\n",
    "    def isToeplitzMatrix(self, matrix):\n",
    "        for i in range(1,len(matrix)):\n",
    "            for j in range(1,len(matrix[0])):\n",
    "                if matrix[i-1][j-1] != matrix[i][j]:\n",
    "                    return False\n",
    "        return True"
   ]
