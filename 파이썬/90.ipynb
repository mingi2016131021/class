   "source": [
    "class Solution(object):\n",
    "    def intersection(self, nums):\n",
    "        results=set(nums[0])\n",
    "        for num in nums:\n",
    "            results = results & set(num)\n",
    "        results =  list(results)\n",
    "        results.sort() \n",
    "        return results"
   ]
