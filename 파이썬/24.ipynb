   "source": [
    "class Solution:\n",
    "\t\tdef restoreString(self, s: str, indices):\n",
    "\t\t\tl1 = [0]*len(s)\n",
    "\n",
    "\t\t\tfor i  in range(0, len(s)):\n",
    "\t\t\t\tl1[indices[i]] = s[i]\n",
    "\n",
    "\t\t\treturn \"\".join(l1)"
   ]
