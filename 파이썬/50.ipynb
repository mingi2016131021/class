    "class Solution(object):\n",
    "    def heightChecker(self, heights):\n",
    "        count=0\n",
    "        s=sorted(heights)\n",
    "        for i in range(len(heights)):\n",
    "            if s[i]!=heights[i]:\n",
    "                count+=1\n",
    "        return count"
   ]
