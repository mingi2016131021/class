   "source": [
    "class Solution(object):\n",
    "    def relativeSortArray(self, arr1, arr2):\n",
    "        l = []\n",
    "        for i in arr2:\n",
    "            while i in arr1:\n",
    "                l.append(i)\n",
    "                arr1.remove(i)\n",
    "        return l + sorted(arr1)"
   ]
