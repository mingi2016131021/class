   "source": [
    "class Solution(object):\n",
    "    def minCostToMoveChips(self, position):\n",
    "        odd = [i for i in position if i%2!=0]\n",
    "        even = [i for i in position if i%2==0]\n",
    "        return min(len(odd), len(even))"
   ]
