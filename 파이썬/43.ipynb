   "source": [
    "class Solution:\n",
    "    def findComplement(self, num):\n",
    "        s=bin(num).replace('0b','')\n",
    "        t=''\n",
    "        for i in s:\n",
    "            if i=='0':\n",
    "                t+='1'\n",
    "            else:\n",
    "                t+='0'\n",
    "        return int(t,2)"
   ]
