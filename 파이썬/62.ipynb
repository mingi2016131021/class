   "source": [
    "class Solution:\n",
    "    def minCostClimbingStairs(self, cost):\n",
    "        n = len(cost)\n",
    "        dp = [0] * n\n",
    "        dp[0] = cost[0]\n",
    "        dp[1] = cost[1]\n",
    "\n",
    "        for i in range(2, n):\n",
    "            dp[i] = min(dp[i-1], dp[i-2]) + cost[i]\n",
    "\n",
    "        return min(dp[n-1], dp[n-2])"
   ]
