    "class Solution(object):\n",
    "     def smallerNumbersThanCurrent(self, nums):\n",
    "        l1=[]\n",
    "        s=sorted(nums)\n",
    "        for i in nums:\n",
    "            \n",
    "            l1.append(s.index(i))\n",
    "        return l1"
   ]
