   "source": [
    "class Solution(object):\n",
    "    def countEven(self, num):\n",
    "        count = 0\n",
    "        for i in range(1,num+1):\n",
    "            x = sum([int(char) for char in str(i)])\n",
    "            if x %2 == 0:\n",
    "                count+=1\n",
    "        return count"
   ]
