    "class Solution:\n",
    "    def largestOddNumber(self, s):\n",
    "        n = len(s)\n",
    "\n",
    "        for i in range(n - 1, -1, -1):\n",
    "            a = int(s[i])\n",
    "            if a % 2 != 0:\n",
    "                return s[:i + 1]\n",
    "\n",
