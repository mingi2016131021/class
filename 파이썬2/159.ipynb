   "source": [
    "class Solution(object):\n",
    "    def fairCandySwap(self, aliceSizes, bobSizes):\n",
    "        diff=(sum(aliceSizes)-sum(bobSizes))//2\n",
    "        for b in bobSizes:\n",
    "            if diff+b in aliceSizes:\n",
    "                return [diff+b,b]      "
   ]
