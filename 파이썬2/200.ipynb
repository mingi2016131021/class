    "class Solution:\n",
    "    def checkIfPangram(self, sentence): \n",
    "        s=\"abcdefghijklmnopqrstuvwxyz\"\n",
    "        for i in s:\n",
    "            if i not in sentence:\n",
    "                return False\n",
    "        return True"
