   "source": [
    "class Solution(object):\n",
    "    def arrayPairSum(self, nums):\n",
    "        nums.sort()\n",
    "        n = len(nums)\n",
    "        ans = 0\n",
    "        for i in range(0, n, 2):\n",
    "            ans += nums[i]\n",
    "        return ans"
   ]
