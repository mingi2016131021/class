    "class Solution(object):\n",
    "    def minMovesToSeat(self, seats, students):\n",
    "        x=sorted(students)\n",
    "        y=sorted(seats)\n",
    "        z=0\n",
    "        for i in range(0,len(seats)):\n",
    "            z+=abs(x[i]-y[i])\n",
    "            #print(z)\n",
    "        return z"
   ]
