   "source": [
    "class Solution:\n",
    "    def countBits(self, n):\n",
    "        dp=[0]\n",
    "        for i in range(1,n+1):\n",
    "            if i%2==1:\n",
    "                dp.append(dp[i-1]+1)\n",
    "            else:\n",
    "                dp.append(dp[i//2])\n",
    "        return dp             "
   ]
