   "source": [
    "class Solution:\n",
    "    def firstBadVersion(self, n):\n",
    "        low, high= 0, n\n",
    "        while low < high:\n",
    "            mid= low+ (high- low)//2\n",
    "            if isBadVersion(mid):\n",
    "                high= mid\n",
    "            else:\n",
    "                low= mid+1\n",
    "        return low"
   ]
