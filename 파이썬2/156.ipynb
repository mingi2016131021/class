   "source": [
    "class Solution(object):\n",
    "    def maxProfit(self, prices):\n",
    "    \tbest = 0\n",
    "    \tbuy = prices[0]\n",
    "    \tfor i in range(1, len(prices)):\n",
    "    \t\tprofit = prices[i] - buy\n",
    "    \t\tbest = max(best, profit)\n",
    "    \t\tbuy = min(buy, prices[i])\n",
    "    \treturn best"
   ]
