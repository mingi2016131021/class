   "source": [
    "class Solution(object):\n",
    "    def areAlmostEqual(self, s1, s2):\n",
    "        c=0\n",
    "        for i,j in zip(s1,s2):\n",
    "            if i!=j:\n",
    "                c+=1\n",
    "        return s1==s2 or sorted(s1)==sorted(s2) and c==2"
   ]
