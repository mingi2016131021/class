   "source": [
    "class Solution:\n",
    "    def kWeakestRows(self, mat, k):\n",
    "        tmp = []\n",
    "        for i, m in enumerate(mat):\n",
    "            cand = (sum(m), i)\n",
    "            tmp.append(cand)\n",
    "        tmp.sort()\n",
    "        return [i[1] for i in tmp[:k]]"
   ]
