    "class Solution(object):\n",
    "    def theMaximumAchievableX(self, num, t):\n",
    "        n1=num-t\n",
    "        n2=num+t\n",
    "        t1=n1-t\n",
    "        t2=n2+t\n",
    "        if t1>t2:\n",
    "            return t1\n",
    "        else:\n",
    "            return t2"
