   "source": [
    "class Solution(object):\n",
    "    def largestAltitude(self, gain):\n",
    "        alt = 0\n",
    "        s = 0\n",
    "        for i in range(len(gain)):\n",
    "            s += gain[i]\n",
    "            if(s > 0):\n",
    "                alt = max(alt, s)\n",
    "        return alt"
   ]
