    "class Solution(object):\n",
    "    def countConsistentStrings(self, allowed, words):\n",
    "        s=0\n",
    "        for x in words:\n",
    "            if set(x).issubset(set(allowed)):\n",
    "                s=s+1\n",
    "        return s "
