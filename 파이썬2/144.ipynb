   "source": [
    "class Solution(object):\n",
    "    def isSubsequence(self, s, t):\n",
    "        j = 0\n",
    "        for i in range(0, len(t)):\n",
    "            if j >= len(s):\n",
    "                break\n",
    "            if s[j] == t[i]:\n",
    "                j += 1\n",
    "        return j == len(s)"
   ]
