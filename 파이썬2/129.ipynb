   "source": [
    "class Solution:\n",
    "    def pivotIndex(self, nums):\n",
    "        left, right  = 0, sum(nums)\n",
    "        for index, num in enumerate(nums):\n",
    "            right -= num\n",
    "            if left == right:\n",
    "                return index\n",
    "            left += num\n",
    "        return -1"
   ]
