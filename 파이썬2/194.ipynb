   "source": [
    "class Solution(object):\n",
    "    def sortSentence(self, s):\n",
    "        res = s.split(\" \")\n",
    "        res.sort(key=lambda x: x[-1])\n",
    "        res = [word[0:-1] for word in res]\n",
    "        \n",
    "        return ' '.join(res)"
   ]
